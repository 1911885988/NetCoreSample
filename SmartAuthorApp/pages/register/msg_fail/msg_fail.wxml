<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="warn" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{title}}</view>
            <view class="weui-msg__desc">{{content}}<!--<navigator url="" class="weui-msg__link">文字链接</navigator>-->
            </view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button bindtap='historyBack' class="weui-btn" type="default">返回</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__text">Copyright © 2008-2016 开卷信息技术有限公司</view>
            </view>
        </view>
    </view>
</view>